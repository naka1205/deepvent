{
  "name": "deepvent",
  "version": "1.2.0",
  "description": "A TypeScript 3D scene engine based on Three.js for ventilation simulation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "optionalDependencies": {
    "dxf-parser": "^1.1.2",
    "libredwg-ts": "^1.0.1"
  },
  "peerDependencies": {
    "three": ">=0.150.0"
  }
}